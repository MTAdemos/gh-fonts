%!
%%BeginFont: Breath-02

%Based on Breath-01. but with adjustments to allow curving or straight neumes

systemdict/currentpacking known
  {userdict/SavePacking currentpacking put true setpacking
}if

11 dict begin

/FontName/Breath-02 def
/FontType 3 def
/FontMatrix[0.001 0 0 0.001 0 0]def
/FontBBox[-500 -2000 2500 2000]def

[
/CharacterDefs 100
/ExtraCharacterDefs 10
/Tools 30
/LegerDict 10
/dstem_length_dict 4
counttomark 2 idiv{dict 2 copy def exch/DictName exch put}repeat pop

Tools begin
  /NeumeWidth 250 def % this is the default width/height for a square neume

  /DiamondWidth 180 NeumeWidth mul 250 div def

  /NeumeWidth_0_rlineto{NeumeWidth 0 rlineto}bind def
  /NeumeWidth_neg_0_rlineto{NeumeWidth neg 0 rlineto}bind def



  /NeumeVstrokeWidth 40 def % width of vertical lines
  /lsb 100 def

  /scd{ % proc to sort out width/setcachedevice, argument is width of filled area
    /widthfilled exch def
    lsb 2 mul widthfilled add   % h disp
    0 % v disp
    lsb -1000 lsb widthfilled add 1200 % setcachedevice
        pop pop pop pop setcharwidth
  }bind def

  /.onround{transform round exch round exch itransform}bind def
  /.ondround{dtransform round exch round exch idtransform}bind def
  /Pm{//.onround exec moveto}bind def

  /wmtx matrix def
  /EffectiveNeumeWidth NeumeWidth def % cludge to allow narrow horozontals where needed

  /widestroke{
    wmtx currentmatrix pop
      1 setlinewidth true setstrokeadjust
      NeumeVstrokeWidth EffectiveNeumeWidth scale stroke
    wmtx setmatrix
    /EffectiveNeumeWidth NeumeWidth store
  }bind def


  /DiamondNh{
    gsave
      DiamondWidth dup scale
      0.66 1 rlineto 0.66 -1 rlineto -0.66 -1 rlineto closepath fill
    grestore
  }bind def

  % dstem_length computes the length of a downstem, working
  % from number of spaces (= staff line separations) in a
  % font with NeumeWidth=400

  /dstem_length{
    dstem_length_dict begin
      /dstem_length_arg exch def
      /Effective_NeumeWidth NeumeWidth 2 mul 400 add 3 div def

      dstem_length_arg Effective_NeumeWidth mul %desired length
      200 div round 200 mul                     %adjusted to nearest half staff sep
    end
  }bind def

  /liquescent_tail{
    NeumeVstrokeWidth 2 div neg 0 rmoveto
    NeumeWidth 400 div dup scale
    0 -200 rmoveto currentpoint newpath moveto
    400 0
    480 -180
    500 -200 rcurveto
    20 -60 40 -40 20 20 rcurveto
    -10 20 -100 580 -520 580 rcurveto
    closepath fill
  }bind def

  /pes1_proc{
    NeumeWidth 285 gt{/EffectiveNeumeWidth 285 store}if

    lsb 200 NeumeWidth EffectiveNeumeWidth sub 2 div sub moveto
    NeumeWidth_0_rlineto
    0 400 rlineto
    0 NeumeWidth EffectiveNeumeWidth sub rlineto
    NeumeWidth_neg_0_rlineto
    NeumeWidth 285 gt{/EffectiveNeumeWidth 285 store}if widestroke
  }bind def


  /n16_proc{
    /OnLine exch def
    % NB two different definitions -- if NeumeWidth much above 200 then
    % the bottom has to drop to the staffline below.

    % if the character sits on a staff line then the top is moved down a little and vice versa

    gsave

      NeumeWidth 220 lt{

        NeumeWidth 160 gt{/EffectiveNeumeWidth 160 store}if

        lsb 200 NeumeWidth EffectiveNeumeWidth sub 2 div sub moveto
        NeumeWidth_0_rlineto
        0 200 rlineto
        0 NeumeWidth EffectiveNeumeWidth sub rlineto
        NeumeWidth_neg_0_rlineto
        NeumeWidth 160 gt{/EffectiveNeumeWidth 160 store}if widestroke
        }
        {

        lsb 60 moveto NeumeWidth_0_rlineto
        /dy0 190 def  % width of bottom blob
        /dy1  85 def  % width of gap
        /dy2 185 def  % width of top blob


        0 dy0 dy1 add dy2 add rlineto
        NeumeWidth_neg_0_rlineto
        0 dy2 neg rlineto
        NeumeWidth NeumeVstrokeWidth sub dup 0 rlineto
        0 dy1 neg rlineto
        neg 0 rlineto
        closepath
        fill
      }ifelse
    grestore
  }bind def
end % Tools

